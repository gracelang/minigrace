#! /bin/bash -f

absolutePath(){
    [[ -d $1 ]] && { cd "$1"; echo "$(pwd -P)"; } ||
    ( cd "$(dirname "$1")" || exit 1; echo "$(pwd -P)/$(basename "$1")"; )
}

HERE=`absolutePath $0`
HERE=`dirname ${HERE}`
echo "${HERE}/js/grace ${HERE}/js/compiler.js $*"
${HERE}/js/grace ${HERE}/js/compiler.js $*
